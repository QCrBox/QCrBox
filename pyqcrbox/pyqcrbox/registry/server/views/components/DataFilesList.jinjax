{#def data_files = [] #}

<div class="block">
  <div id="data_files_list" class="list">
    {% if data_files %}
      {% for data_file in data_files %}
      <div class="list-item">
        <li><code class="filename">{{ data_file.filename }}</code></li>
      </div>
      {% endfor %}
    {% else %}
      No data files found.
    {% endif %}
  </div>
</div>
